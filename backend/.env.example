# 数据库配置
DB_URL=jdbc:mysql://localhost:3306/owner_voting?useUnicode=true&characterEncoding=utf8&serverTimezone=Asia/Shanghai
DB_USERNAME=owner
DB_PASSWORD=your_secure_password_here

# JPA配置
JPA_DDL_AUTO=validate
JPA_SHOW_SQL=false
JPA_FORMAT_SQL=false

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_here
REDIS_DATABASE=0
REDIS_TIMEOUT=10000

# JWT配置
JWT_SECRET_KEY=your_very_secure_jwt_secret_key_here_at_least_32_characters
JWT_EXPIRE_LENGTH=3600000

# 注意：
# 1. 复制此文件为 .env 并填入实际的配置值
# 2. 确保 .env 文件已添加到 .gitignore 中
# 3. JWT密钥应至少32个字符，建议使用随机生成的强密钥
# 4. 生产环境中请使用强密码